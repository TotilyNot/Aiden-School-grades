function initGrading() 
{
	initScroll();
}

function showNote(idKey)
{
	var noteDiv = document.getElementById('pssClassNote');
	var htmlText = remote.getNote(idKey);
	if (htmlText != null)
	{
		noteDiv.innerHTML = htmlText;
		noteDiv.style.display = 'block';
	}
}

function provideRowFunction(cellId, idKey)
{
	document.getElementById('gradeBookKey').value = idKey;
	if (cellId != null && cellId.endsWith('td2'))
	{
		setActionForFormAndSubmit('PSSEmailTeacherAction.do');
	}
	else if (cellId != null && cellId.endsWith('td0'))
	{
		showNote(idKey);
	}
}

function onGridCellClick(a)
{
	var addedSkillbook = remote.isAddedSkillbook();

	var cellId = a.parentNode.id;
	var idKey = a.id;
	document.getElementById('gradeBookKey').value = idKey;
	if (cellId != null && cellId.endsWith('td2'))
	{
		setActionForFormAndSubmit('PSSEmailTeacherAction.do');
	}
	else if (cellId != null && cellId.endsWith('td0'))
	{
		showNote(idKey);
	}
	else if (addedSkillbook && cellId != null && cellId.endsWith('td6'))
	{
		setActionForFormAndSubmit('PSSStudentSkillbookAction.do');
	}
	else
	{
		setActionForFormAndSubmit('PSSViewGradeBookEntriesAction.do');
	}
}
</script>

